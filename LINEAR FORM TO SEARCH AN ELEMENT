#include<stdio.h>
int main(){
int arr[100],i,size,value;
printf("enter size of array:");
scanf("%d",&size);
printf("enter the element:");
for(i=0;i<size;i++){
scanf("%d",&arr[i]);
}
printf("enter the searching number:");
scanf("%d",&value);
for(i=0;i<size;i++){
if(arr[i]==value){
printf("%d,the value is present is position of %d",value,i+1);
}
}
}
