}
int main(){
int arr[]={11,83,20,75,44};
int n=sizeof(arr)/sizeof(arr[0]);
printf("original array:");
for(int i=0;i<n;i++){
printf("%d",arr[i]);
}
printf("\n");
insertionsort(arr,n);
}
