#include<stdio.h>
int main(){
	int arr[50],n,pos,i,ele;
	printf("enter the size of array:");
	scanf("%d",&n);
	printf("enter the element in array:");
	for(i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	printf("enter the position of element:");
	scanf("%d",&pos);
	printf("enter the value:");
	scanf("%d",&ele);
	for(i=n;i>=pos;i--){
		arr[i]=arr[i-1];
	}
		arr[pos]=ele;
		n++;
	
	printf("final array:");
	for(i=0;i<n;i++){
		printf("%d",arr[i]);
	}}
